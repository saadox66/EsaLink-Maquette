{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/components/CertifCard.jsx", "src/app/components/card.tsx", "src/app/components/cardSlider.tsx", "src/app/components/certificationPDP.tsx", "src/app/components/CertifSection.tsx", "src/app/components/satisfactions.tsx", "src/app/components/section1.tsx", "src/app/components/section2.tsx", "src/app/components/section3.tsx", "src/app/components/teckStack.tsx"],
  "exclude": ["node_modules"]
}
